:hasArchivalReferenceCode rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf core:hasIdentifier ;
                          rdfs:comment "Questa proprietà mette in relazione la risorsa archivistica con la sua segnatura."@it ,
                                       "This property relates the archival resource to its archival reference code."@en ;
                          rdfs:isDefinedBy <https://w3id.org/arco/archives/ontology/archival-resource> ;
                          rdfs:label "ha segnatura archivistica"@it ,
                                     "has archival reference code"@en ;
                          owl:versionInfo "instabile"@it ,
                                          "unstable"@en .


