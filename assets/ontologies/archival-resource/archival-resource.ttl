###  https://w3id.org/arco/archives/ontology/archival-resource/hasCentury
:hasCentury rdf:type owl:ObjectProperty ;
            rdfs:domain :TimeInstant ;
            rdfs:range :Century ;
            rdfs:comment "Questa proprietà mette in relazione una risorsa ed una entità temporale di tipo secolo."@it ,
                         "This property relates a resource to a century-type time entity."@en ;
            rdfs:isDefinedBy <https://w3id.org/arco/archives/ontology/archival-resource> ;
            rdfs:label "ha secolo"@it ,
                       "has century"@en ;
            owl:versionInfo "instabile"@it ,
                            "unstable"@en .


###  https://w3id.org/arco/archives/ontology/archival-resource/hasChild
:hasChild rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf core:isAssociatedWith ;
          owl:inverseOf :hasParent ;
          rdfs:domain :HistoricalOrInstitutionalContext ;
          rdfs:range :HistoricalOrInstitutionalContext ;
          rdfs:comment "Questa proprietà mette in relazione una qualsiasi entità con un'altra entità gerarchicamente inferiore. In questo caso particolare mette in relazione il contesto storico-istituzionale con un altro contesto storico-istituzionale gerarchicamente inferiore."@it ,
                       "This property relates any entity to another hierarchically inferior entity. In this particular case, it relates the historical-institutional context to another hierarchically inferior historical-institutional context."@en ;
          rdfs:isDefinedBy <https://w3id.org/arco/archives/ontology/archival-resource> ;
          rdfs:label "ha figlio"@it ,
                     "has child"@en ;
          owl:versionInfo "instabile"@it ,
                          "unstable"@en .


