:hasAttributeInTime rdf:type owl:ObjectProperty ;
                    rdfs:domain :NaturalPerson ;
                    rdfs:range :AttributeInTime ;
                    rdfs:comment "Questa proprietà mette in relazione la classe AttributeInTime con una risorsa archivistica o una persona"@it ,
                                 "This property relates the AttributeInTime class to an archive resource or person."@en ;
